<application>
  <component name="ProjectManager">
    <defaultProject>
      <component name="Git.Settings">
        <option name="PUSH_AUTO_UPDATE" value="true" />
      </component>
      <component name="JsBowerSettings">
        <node-interpreter value="project" />
        <bower-package value="" />
        <bower.json value="" />
      </component>
      <component name="JsFlowSettings">
        <service-enabled>true</service-enabled>
        <exe-path />
        <other-services-enabled>true</other-services-enabled>
        <auto-save>true</auto-save>
      </component>
      <component name="ProjectDictionaryState">
        <dictionary name="Daniel" />
      </component>
      <component name="ProjectInspectionProfilesVisibleTreeState">
        <entry key="Project Default">
          <profile-state>
            <expanded-state>
              <State />
            </expanded-state>
            <selected-state>
              <State>
                <id>Angular</id>
              </State>
            </selected-state>
          </profile-state>
        </entry>
      </component>
      <component name="PropertiesComponent">
        <property name="node.js.path.for.package.eslint" value="project" />
        <property name="node.js.path.for.package.standard" value="project" />
        <property name="node.js.path.for.package.stylelint" value="project" />
        <property name="node.js.selected.package.eslint" value="(autodetect)" />
        <property name="node.js.selected.package.standard" value="" />
        <property name="node.js.selected.package.stylelint" value="" />
        <property name="nodejs_interpreter_path" value="node" />
        <property name="nodejs_package_manager_path" value="npm" />
        <property name="run.prettier.on.save" value="true" />
        <property name="settings.editor.selected.configurable" value="settings.saveactions" />
        <property name="typescript.add.unambiguous.imports.on.the.fly" value="true" />
      </component>
      <component name="SaveActionSettings">
        <option name="actions">
          <set>
            <option value="activate" />
            <option value="organizeImports" />
          </set>
        </option>
      </component>
      <component name="TypeScriptCompiler">
        <option name="enableServiceForProjectsWithoutConfig" value="false" />
      </component>
      <component name="VagrantProjectSettings">
        <option name="instanceFolder" value="" />
        <option name="provider" value="" />
      </component>
      <component name="YarnPnpManager">
        <pnp-files />
      </component>
      <component name="jwt">
        <option name="rs256PrivateKeyFile" value="" />
        <option name="rs256PublicKeyFile" value="" />
      </component>
      <component name="masterDetails">
        <states>
          <state key="ScopeChooserConfigurable.UI">
            <settings>
              <splitter-proportions>
                <option name="proportions">
                  <list>
                    <option value="0.2" />
                  </list>
                </option>
              </splitter-proportions>
            </settings>
          </state>
        </states>
      </component>
    </defaultProject>
  </component>
</application>